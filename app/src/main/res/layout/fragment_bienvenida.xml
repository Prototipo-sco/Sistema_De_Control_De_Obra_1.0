<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/White"
    tools:context="com.ic.sexto.sco.fragments.BienvenidaFragment">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="336dp"
        android:layout_height="280dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="27dp"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:src="@drawable/logo_mod2" />

    <TextView
        android:id="@+id/id_nombre_contenido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="380dp"
        android:text="Arq. Reyes Leobardo Martinez Guzman"
        android:layout_centerHorizontal="true"
        android:textSize="18dp"
        android:textColor="@color/colorChido" />

</RelativeLayout>
